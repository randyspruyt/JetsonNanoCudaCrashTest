{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GDB Remote Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/bin/mystique",
            "cwd": "${workspaceRoot}",
            "miDebuggerServerAddress": "nano-randy:222",
            "linux": {
                "miDebuggerPath": "Z:/MystiquePipelinegdb/gdb/aarch64-linux-gnu-gdb",
            },
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                } 
            ],
        }
    ]
}